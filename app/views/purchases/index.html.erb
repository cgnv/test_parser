<p id="notice"><%= notice %></p>

<h1>Purchases</h1>

<table>
  <thead>
    <tr>
      <th>Notification</th>
      <th>Purchasenumber</th>
      <th>Docpublishdate</th>
      <th>Href</th>
      <th>Purchaseobjectinfo</th>
      <th>Guid</th>
      <th>Createdatetime</th>
      <th>Registrationnumber</th>
      <th>Name</th>
      <th>Purchasemethodcode</th>
      <th>Purchasecodename</th>
      <th>Publicationdatetime</th>
      <th>Status</th>
      <th>Version</th>
      <th>Modificationdate</th>
      <th>Saveuserid</th>
      <th>Examinationplace</th>
      <th>Examinationdatetime</th>
      <th>Submissionclosedatetime</th>
      <th>Publicationplanneddate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @purchases.each do |purchase| %>
      <tr>
        <td><%= purchase.notification_id %></td>
        <td><%= purchase.purchaseNumber %></td>
        <td><%= purchase.docPublishDate %></td>
        <td><%= purchase.href %></td>
        <td><%= purchase.purchaseObjectInfo %></td>
        <td><%= purchase.guid %></td>
        <td><%= purchase.createDateTime %></td>
        <td><%= purchase.registrationNumber %></td>
        <td><%= purchase.name %></td>
        <td><%= purchase.purchaseMethodCode %></td>
        <td><%= purchase.purchaseCodeName %></td>
        <td><%= purchase.publicationDateTime %></td>
        <td><%= purchase.status %></td>
        <td><%= purchase.version %></td>
        <td><%= purchase.modificationDate %></td>
        <td><%= purchase.saveUserId %></td>
        <td><%= purchase.examinationPlace %></td>
        <td><%= purchase.examinationDateTime %></td>
        <td><%= purchase.submissionCloseDateTime %></td>
        <td><%= purchase.publicationPlannedDate %></td>
        <td><%= link_to 'Show', purchase %></td>
        <td><%= link_to 'Edit', edit_purchase_path(purchase) %></td>
        <td><%= link_to 'Destroy', purchase, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Purchase', new_purchase_path %>
